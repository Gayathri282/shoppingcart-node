    <section>
        <div class="container mt-3 text-center m-auto ">
            <div class="row ">
              {{#each products}}
                <div class="col-md-3 ">
                    <div class="card" style="width: 17rem;">
                    <img src="/productImages/{{this._id}}.jpg" class="card-img-top m-auto mt-2" alt="..." style="width:250px;height:200px;">
                    <div class="card-body">
                        <h5 class="card-title">{{this.name}}</h5>
                        <p class="card-text">{{this.description}}</p>
                        <a href="#" class="btn btn-primary">${{this.price}}</a>
                    </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </section>

   