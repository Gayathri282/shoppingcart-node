<section>
    <div class="container">
        <table class="table">
            <p>User ID:{{user._id}}</p>
            <thead>
                <tr>
                    <th scope="col">Item</th>
                    <th scope="col">Title</th>
                     <th scope="col">Price</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                <tr id="row_{{this.products._id}}">
                    {{!-- <td>id:{{this.products._id}}</td> --}}
                    <td><img src="/productImages/{{this.products._id}}.jpg" alt="" style="width:100px;height:100px"></td>
                    <td><p>{{this.products.name}}</p></td>
                    <td><p>&#8377;{{this.products.price}}</p></td>
                   
                    {{!-- <td><p>{{parseInt this.products.price}}*{{ parseInt this.quantity}}</p></td> --}}
                    <td>
                            <button onclick="addToCartFromWishlist('{{this.products._id}}')" class="btn btn-primary mr-5">Add to Cart</button>
                            {{!-- <a href="/deleteProduct?cartId={{this._id}}&productId={{this.products._id}}" class="btn btn-danger" onclick="return confirm('Do you want to delete {{this.name}}')">X</a> --}}
                            </td>
                        <td>
                             <button onclick=" deleteFromWishlist('{{this._id}}','{{this.products._id}}','{{user._id}}')" class="btn btn-danger">X</button>
                            {{!-- <a href="/deleteProduct?cartId={{this._id}}&productId={{this.products._id}}" class="btn btn-danger" onclick="return confirm('Do you want to delete {{this.name}}')">X</a> --}}
                            </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        <hr class="mt-4"/>
        
    </div>
</section>
